<div class="container">
  <br />
  <div class="jumbotron">
    <h1><%= @quote.full_name %></h1>
      <dl class="dl-horizontal">
        <dt>Phone</dt>
        <dd><%= @quote.phone %></dd>
        <dt>Email</dt>
        <dd><%= @quote.email %></dd>
        <dt>Business Name</dt>
        <dd><%= @quote.business_name %></dd>
        <dt>State</dt>
        <dd><%= @quote.state %></dd>
        <dt>Brick and Mortar Business Type</dt>
        <dd><%= @quote.bm_business_type %></dd>
        <dt>ECOMM Business Type</dt>
        <dd><%= @quote.ecomm_business_type %></dd>
        <dt>Payment Methods</dt>
        <dd><%= @quote.payment_methods %></dd>
        <dt>Annual Processing Volume</dt>
        <dd><%= @quote.annual_average_processing_volume %></dd>
        <dt>Average Sale Ammount</dt>
        <dd><%= @quote.average_sale_amount %></dd>
        <dt>Last Month's Processing Volume</dt>
        <dd><%= @quote.last_month_processing_volume %></dd>
        <dt>Lats Month's Processing Fees</dt>
        <dd><%= @quote.last_month_processing_fees %></dd>
        <dt>Chargebacks</dt>
        <dd><%= @quote.chargebacks %></dd>
      </dl>
      <br />
      <%= link_to "Edit Info", edit_quote_path, class: "btn btn-primary" %>
  </div>
</div>
